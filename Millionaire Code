           answer3 = tk.Button(self, text = answer_1[arr[2]], width = 30, height = 3, fg = "black")
           answer4 = tk.Button(self, text = answer_1[arr[3]], width = 30, height = 3, fg = "black")
           
           if answer1['text'] == answer_1[0]:
               answer1 = tk.Button(self, text = answer_1[arr[0]],
                                   width = 30, height = 3, fg = "black",
                                   command = lambda: controller.show_frame("Q4_screen"))
           else:
               answer1 = tk.Button(self, text = answer_1[arr[0]],
                                   width = 30, height = 3, fg = "black",
                                   command = lambda: controller.show_frame("lose_screen"))                
           if answer2['text'] == answer_1[0]:
               answer2 = tk.Button(self, text = answer_1[arr[1]],
                                   width = 30, height = 3, fg = "black",
                                   command = lambda: controller.show_frame("Q4_screen"))
           else:
               answer2 = tk.Button(self, text = answer_1[arr[1]],
                                   width = 30, height = 3, fg = "black",
                                   command = lambda: controller.show_frame("lose_screen"))                
           if answer3['text'] == answer_1[0]:
               answer3 = tk.Button(self, text = answer_1[arr[2]],
                                   width = 30, height = 3, fg = "black",
                                   command = lambda: controller.show_frame("Q4_screen"))
           else:
               answer3 = tk.Button(self, text = answer_1[arr[2]],
                                   width = 30, height = 3, fg = "black",
                                   command = lambda: controller.show_frame("lose_screen"))
           if answer4['text'] == answer_1[0]:
               answer4 = tk.Button(self, text = answer_1[arr[3]],
                                   width = 30, height = 3, fg = "black",
                                   command = lambda: controller.show_frame("Q4_screen"))
           else:
               answer4 = tk.Button(self, text = answer_1[arr[3]],
                                   width = 30, height = 3, fg = "black",
                                   command = lambda: controller.show_frame("lose_screen"))

               
           answer1.place(x = 40, y = 340)
