class Q5_screen(tk.Frame):
   def __init__(self, parent, controller):
       tk.Frame.__init__(self, parent)
       self.controller = controller
       def file_len(fname):
           with open(fname) as f:
               for count, l in enumerate(f):
                   pass
           return count + 1

       while True:
           i = random.randint(-1, (file_len("Questions.txt")-1))
           if i in answer_arr:
               continue
           else:
               add_list(i)
               break
       a = i
           
       with open("Questions.txt", "r") as f:
           content = f.readlines()[a]
           question_string = content
           
       question_label = tk.Label(self, text = question_string, width = 80, height = 10)
       question_label.place(x = 20, y = 200)

       with open ("Answers.txt", "r") as r:
           file_len("Answers.txt")
           line = r.readlines()[i]
           answer_1 = [word.strip() for word in line.split(',')]
           print(answer_1)

           
           arr = [0,1,2,3]
           random.shuffle(arr)
           
           answer1 = tk.Button(self, text = answer_1[arr[0]], width = 30, height = 3, fg = "black")
           answer2 = tk.Button(self, text = answer_1[arr[1]], width = 30,height = 3, fg = "black")
           answer3 = tk.Button(self, text = answer_1[arr[2]], width = 30, height = 3, fg = "black")
           answer4 = tk.Button(self, text = answer_1[arr[3]], width = 30, height = 3, fg = "black")
           
           if answer1['text'] == answer_1[0]:
               answer1 = tk.Button(self, text = answer_1[arr[0]],
                                   width = 30, height = 3, fg = "black",
                                   command = lambda: controller.show_frame("Q6_screen"))
           else:
               answer1 = tk.Button(self, text = answer_1[arr[0]],
                                   width = 30, height = 3, fg = "black",
                                   command = lambda: controller.show_frame("lose_screen"))                
           if answer2['text'] == answer_1[0]:
               answer2 = tk.Button(self, text = answer_1[arr[1]],
                                   width = 30, height = 3, fg = "black",
                                   command = lambda: controller.show_frame("Q6_screen"))
           else:
               answer2 = tk.Button(self, text = answer_1[arr[1]],
                                   width = 30, height = 3, fg = "black",
                                   command = lambda: controller.show_frame("lose_screen"))                
           if answer3['text'] == answer_1[0]:
               answer3 = tk.Button(self, text = answer_1[arr[2]],
                                   width = 30, height = 3, fg = "black",
                                   command = lambda: controller.show_frame("Q6_screen"))
           else:
               answer3 = tk.Button(self, text = answer_1[arr[2]],
                                   width = 30, height = 3, fg = "black",
                                   command = lambda: controller.show_frame("lose_screen"))
           if answer4['text'] == answer_1[0]:
               answer4 = tk.Button(self, text = answer_1[arr[3]],
                                   width = 30, height = 3, fg = "black",
                                   command = lambda: controller.show_frame("Q6_screen"))
           else:
               answer4 = tk.Button(self, text = answer_1[arr[3]],
                                   width = 30, height = 3, fg = "black",
                                   command = lambda: controller.show_frame("lose_screen"))

               
           answer1.place(x = 40, y = 340)
           answer2.place(x = 40, y = 450)            
           answer3.place(x = 330, y = 340)
           answer4.place(x = 330, y = 450)

