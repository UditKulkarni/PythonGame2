           answer_1 = [word.strip() for word in line.split(',')]
           print(answer_1)

           
           arr = [0,1,2,3]
           random.shuffle(arr)
           
           answer1 = tk.Button(self, text = answer_1[arr[0]], width = 30, height = 3, fg = "black")
           answer2 = tk.Button(self, text = answer_1[arr[1]], width = 30,height = 3, fg = "black")
           answer3 = tk.Button(self, text = answer_1[arr[2]], width = 30, height = 3, fg = "black")
           answer4 = tk.Button(self, text = answer_1[arr[3]], width = 30, height = 3, fg = "black")
           
           if answer1['text'] == answer_1[0]:
               answer1 = tk.Button(self, text = answer_1[arr[0]],
                                   width = 30, height = 3, fg = "black",
                                   command = lambda: controller.show_frame("Q10_screen"))
           else:
               answer1 = tk.Button(self, text = answer_1[arr[0]],
                                   width = 30, height = 3, fg = "black",
                                   command = lambda: controller.show_frame("lose_screen"))                
           if answer2['text'] == answer_1[0]:
               answer2 = tk.Button(self, text = answer_1[arr[1]],
                                   width = 30, height = 3, fg = "black",
                                   command = lambda: controller.show_frame("Q10_screen"))
           else:
               answer2 = tk.Button(self, text = answer_1[arr[1]],
                                   width = 30, height = 3, fg = "black",
                                   command = lambda: controller.show_frame("lose_screen"))                
           if answer3['text'] == answer_1[0]:
               answer3 = tk.Button(self, text = answer_1[arr[2]],
                                   width = 30, height = 3, fg = "black",
                                   command = lambda: controller.show_frame("Q10_screen"))
           else:
               answer3 = tk.Button(self, text = answer_1[arr[2]],
                                   width = 30, height = 3, fg = "black",
                                   command = lambda: controller.show_frame("lose_screen"))
           if answer4['text'] == answer_1[0]:
               answer4 = tk.Button(self, text = answer_1[arr[3]],
                                   width = 30, height = 3, fg = "black",
                                   command = lambda: controller.show_frame("Q10_screen"))
           else:
               answer4 = tk.Button(self, text = answer_1[arr[3]],
                                   width = 30, height = 3, fg = "black",
                                   command = lambda: controller.show_frame("lose_screen"))

               
           answer1.place(x = 40, y = 340)
           answer2.place(x = 40, y = 450)            
           answer3.place(x = 330, y = 340)
           answer4.place(x = 330, y = 450)

#---------------------------Earnings----------------------------------------

       label1 = tk.Label(self, text = "$1,000,000", width = 25, height = 2,  bg = "blue")
       label2 = tk.Label(self, text = "$750,000", width = 25, height = 2, bg = "yellow")
       label3 = tk.Label(self, text = "$500,000", width = 25, height = 2, bg = "grey")
       label4 = tk.Label(self, text = "$250,000", width = 25, height = 2, bg = "grey")
       label5 = tk.Label(self, text = "$100,000", width = 25, height = 2, bg = "grey")
       label6 = tk.Label(self, text = "$20,000", width = 25, height = 2, bg = "grey")
       label7 = tk.Label(self, text = "$5000", width = 25, height = 2, bg = "grey")
       label8 = tk.Label(self, text = "$1000", width = 25, height = 2, bg = "grey")
       label9 = tk.Label(self, text = "$500", width = 25, height = 2, bg = "grey")
       label10 = tk.Label(self, text = "$100", width = 25, height = 2, bg = "grey")

       label1.place(x = 600, y = 50)
       label2.place(x = 600, y = 85)
       label3.place(x = 600, y = 120)
       label4.place(x = 600, y = 155)
       label5.place(x = 600, y = 190)
       label6.place(x = 600, y = 225)
       label7.place(x = 600, y = 260)
       label8.place(x = 600, y = 295)
       label9.place(x = 600, y = 330)
       label10.place(x = 600, y = 365)
       
           
#--------------------------------------------------------------------------

class Q10_screen(tk.Frame):
   def __init__(self, parent, controller):
       tk.Frame.__init__(self, parent)
       self.controller = controller
       def file_len(fname):
           with open(fname) as f:
               for count, l in enumerate(f):
                   pass
           return count + 1

       while True:
           i = random.randint(-1, (file_len("Questions.txt")-1))
           if i in answer_arr:
               continue
           else:
               add_list(i)
               break
       a = i
           
       with open("Questions.txt", "r") as f:
           content = f.readlines()[a]
           question_string = content
           
       question_label = tk.Label(self, text = question_string, width = 80, height = 10)
       question_label.place(x = 20, y = 200)

       with open ("Answers.txt", "r") as r:
           file_len("Answers.txt")
           line = r.readlines()[i]
           answer_1 = [word.strip() for word in line.split(',')]
           print(answer_1)

           
           arr = [0,1,2,3]
           random.shuffle(arr)
           
           answer1 = tk.Button(self, text = answer_1[arr[0]], width = 30, height = 3, fg = "black")
           answer2 = tk.Button(self, text = answer_1[arr[1]], width = 30,height = 3, fg = "black")
           answer3 = tk.Button(self, text = answer_1[arr[2]], width = 30, height = 3, fg = "black")
           answer4 = tk.Button(self, text = answer_1[arr[3]], width = 30, height = 3, fg = "black")
           
           if answer1['text'] == answer_1[0]:
               answer1 = tk.Button(self, text = answer_1[arr[0]],
                                   width = 30, height = 3, fg = "black",
                                   command = lambda: controller.show_frame("win_screen"))
           else:
               answer1 = tk.Button(self, text = answer_1[arr[0]],
                                   width = 30, height = 3, fg = "black",
                                   command = lambda: controller.show_frame("lose_screen"))                
           if answer2['text'] == answer_1[0]:
               answer2 = tk.Button(self, text = answer_1[arr[1]],
                                   width = 30, height = 3, fg = "black",
                                   command = lambda: controller.show_frame("win_screen"))
           else:
               answer2 = tk.Button(self, text = answer_1[arr[1]],
                                   width = 30, height = 3, fg = "black",
                                   command = lambda: controller.show_frame("lose_screen"))                
           if answer3['text'] == answer_1[0]:
               answer3 = tk.Button(self, text = answer_1[arr[2]],
                                   width = 30, height = 3, fg = "black",
                                   command = lambda: controller.show_frame("win_screen"))
           else:
               answer3 = tk.Button(self, text = answer_1[arr[2]],
                                   width = 30, height = 3, fg = "black",
                                   command = lambda: controller.show_frame("lose_screen"))
           if answer4['text'] == answer_1[0]:
               answer4 = tk.Button(self, text = answer_1[arr[3]],
                                   width = 30, height = 3, fg = "black",
                                   command = lambda: controller.show_frame("win_screen"))
           else:
               answer4 = tk.Button(self, text = answer_1[arr[3]],
                                   width = 30, height = 3, fg = "black",
                                   command = lambda: controller.show_frame("lose_screen"))

               
           answer1.place(x = 40, y = 340)
           answer2.place(x = 40, y = 450)            
           answer3.place(x = 330, y = 340)
           answer4.place(x = 330, y = 450)

